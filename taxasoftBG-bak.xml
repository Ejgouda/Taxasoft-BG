	<field name="country_id" len='6' type='select' tableNr='6' combine='1' list='1' ancor='1'/>
	<field name="division_id" len='6' type='select' tableNr='7' spaces='           ' dependent='country_id' list='1' ancor='1' />
	<field name="itf_loc" showAs="Locality" type="text" len="120" hig='3'  />
	<field name="itf_alt" showAs="Elevation" len="10"  list='0' ancor='1' combine='1'/>
	<field name="itf_altx" showAs="Max.elevation" len="10"  list='0' ancor='1' spaces="                   " />
	<field name="latitude" len="10"  list='0' ancor='1' combine='1' />
	<field name="longitude" len="10"  list='0' ancor='1' explain='(eg. -102.3405)'/>
	<field name="itf_hab" showAs="Habitat" type="text" len="120" hig='2' />
	<field name="description" type="text" len="120" hig='2' />
	<field name="itf_cnam" showAs="Prim.col." len="20" maxLen="100"  list='20' ancor='1' combine='1' />
	<field name="itf_cid" showAs="Coll.nbr."  len="20"  list='1' ancor='1'  spaces="    " combine='1' />
	<field name="itf_cdat" showAs="Coll.date" len="25"  spaces=" " />
	<field name="itf_nam2" showAs="Additional coll." len="100"  list='0' ancor='1' />
	<field name="identification_id" len='6' type='select' tableNr='3' dependent='accession_id=id' explain='(accepted identification)' />
	<field name="phenology" len="20" maxLen="50" />
	<field name="itf_cnot" showAs="Coll.notes" type="text" len="120" hig='3' />
	<field name="Changed" len='40' />
</table>

<table name='acc_location' description="Accession Location Record" 
	nr='2' fieldNr='22' maxFieldHig='10' sortDefault='garden_location,sequence' linkToTable='1' linkField='accession_id'>	
	<field name="id" len='6' list='1' ancor='1'/>
