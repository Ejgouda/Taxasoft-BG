	<field name="taxon_id" showAs="Parent" len='6' type='select' tableNr='0'/>
	<field name="itf_fam_id" showAs="Family" len='6' type='select' tableNr='24' ancor='1' explain='(double click and start typing)'/>
	<field name="itf_genhyb_id" showAs="Genus hybrid" len='6' type='select' tableNr='13' combine='1'/>
	<field name="itf_sphyb_id" showAs="Species hybrid" len='6' type='select' tableNr='13' combine='1' spaces='         ' />
	<field name="itf_spql_id" showAs="Species qualifier" len='6' type='select' tableNr='20'  spaces='          ' />
	<field name="itf_gen" showAs="Genus" len='20' list='1' ancor='1' index='1'  combine='1'/>
	<field name="itf_sp" showAs="Epithet" len='20' list='1' ancor='1' index='1'  combine='1' spaces='     ' />
	<field name="itf_spau" showAs="Author" len='50' maxLen='60' list='0' spaces='         ' />
	<field name="itf_isprk_id" showAs="is2.rank" len='6' type='select' tableNr='18' list='1' ancor='1' combine='1'/>
	<field name="itf_isp" showAs="is.epithet" len='20' list='1' ancor='1' index='1'  combine='1' spaces='           '/>
	<field name="itf_ispau" showAs="is.author" len='50' maxLen='60' list='0' spaces='      ' />
	<field name="itf_isp2rk_id" showAs="is2.rank" len='6' type='select' tableNr='18' list='1' ancor='1' combine='1'/>
	<field name="itf_isp2" showAs="is2.epithet" len='20' list='1' ancor='1' index='1'  combine='1' spaces='         '/>
	<field name="itf_isp2au" showAs="is2.author" len='50' maxLen='60' list='0' spaces='    ' />
	<field name="itf_cul" showAs="Cultivar" len='20' list='1' ancor='1' index='1' combine='1'/>
	<field name="itf_culgrp" showAs="Cult.group" len='20' list='0' ancor='1' index='1' combine='1'/>
	<field name="itf_trades" showAs="Trade_name" len='20' list='0' ancor='1' index='1' />
	<field name="itf_vernam" showAs="Vernacular name" len='20' list='0' ancor='1' index='1'/>
	<field name="itf_per_id" showAs="Perennation" len='6' type='select' tableNr='15' spaces='                                                       '/>
	<field name="itf_accuse_id" showAs="Use" len='6' type='select' tableNr='23' />
	<field name="type" showAs="Type specimen" type="text" hig='1' len='140'/>
	<field name="publication" type="text" list='0' hig='1' len='140' />
	<field name="literature_id" len='6' type='select' tableNr='5' explain='(Publication reference)' />
	<field name="collation" len='20' list='0'  combine='1'/>
	<field name="itf_georan" showAs="Distribution" type="text" hig='3'  len='140'/>
	<field name="altitude" len='20' />
	<field name="habitat" type="text" hig='1' len='140' />
	<field name="notes" type="text" hig='1' len='140'/>
	<field name="Changed" len='50' />
		
	<function 
		name="WCSP - "
		replace="itf_gen,itf_sp,itf_isprk,itf_isp"
		url="http://apps.kew.org/wcsp/advsearch.do?genus=@itf_gen&amp;species=@itf_sp&amp;infraRank=@itf_isprk&amp;infraEpithet=@itf_isp&amp;page=x" />
</table>

<table 	name='accession' nr='1' description="Accession Specimen Record" 
	fieldNr='22' maxFieldHig='10' sortDefault='itf_accid' linkToTable='0' reference='itf_accid' >	

