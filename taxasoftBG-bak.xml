<table name='taxon' description="Taxon Name Record"
	nr='0' fieldNr='21' maxFieldHig='2' sortDefault="itf_gen,itf_sp,itf_isp2" reference="itf_gen,itf_sp,itf_isp,itf_isp2" >

	<field name="id" len='6' list='1'/>
